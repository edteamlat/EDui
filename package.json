    "style-loader": "^1.0.0",
    "uglify-es": "^3.3.9"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": "^16.7.0",
    "node-sass": "^4.12.0",
    "ed-grid": "^2.9.0",
    "edteam-style-guides": "0.0.59"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
